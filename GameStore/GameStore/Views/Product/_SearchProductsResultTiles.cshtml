@model ICollection<GameStore.ViewModels.ProductListItem>

@if (Model.Count > 0)
{
    <ul class="table-list">
        @foreach (var item in Model)
        {
            @Html.Partial("_ProductBox", item)
        }
    </ul>
}
else
{
    <div class="center-align">Nie znaleziono produktów spełniających wyniki wyszukiwania.</div>
}
